{
    "name": "timkippdev/graphql-starter-project",
    "description": "Starter project for working with the PHP GraphQL library",
    "license": "MIT",
    "keywords": [ "graphql", "starter project" ],
    "authors": [
      {
        "name": "Tim Kipp",
        "email": "timkipp.22.developer@gmail.com"
      }
    ],
    "autoload" : {
      "psr-4" : {
        "App\\" : "src/"
      }
    },
    "autoload-dev" : {
      "psr-4" : {
        "Tests\\" : "tests/"
      }
    },
    "require": {
      "php": "^7.1.3",
      "webonyx/graphql-php": "^0.13.0"
    },
    "require-dev": {
    },
    "config": {
      "discard-changes": true,
      "sort-packages": true
    },
    "prefer-stable": true
  }
